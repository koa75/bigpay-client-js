{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4],
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "max-len": 0,
        "no-param-reassign": ["error", { "props": false }],
        "no-use-before-define": [2, "nofunc"]
    }
}
